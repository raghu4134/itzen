<?xml version="1.0"?>
<mapping>
	<class name="javaitzen.spring.oxm.ClassToXMLMap">
	   <map-to xml="message"/>
	   <field name="data" type="string">
	      <bind-xml name="data" node="element"/>
	   </field>
	   <field name="history" type="string">
	      <bind-xml name="history" node="element"/>
	   </field>
	</class>
</mapping>